{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-zip": "*",
        "creativeorange/gravatar": "^1.0",
        "cs/wrapper_dhl": "dev-master",
        "dirape/token": "^2.3",
        "guzzlehttp/guzzle": "^7.0.1",
        "hedii/laravel-gelf-logger": "^8.1",
        "jenssegers/mongodb": "*",
        "kalnoy/nestedset": "^6.0",
        "kyslik/column-sortable": "^6.3",
        "labs64/netlicensingclient-php": "^2.4",
        "laravel/framework": "^10.0",
        "laravel/passport": "^12.2",
        "laravel/tinker": "^2.7",
        "lcobucci/jwt": "^4.0",
        "mongodb/mongodb": "^1.9",
        "monolog/monolog": "^3.0",
        "sainsburys/guzzle-oauth2-plugin": "^3.0",
        "spatie/laravel-permission": "^5.5",
        "tenantcloud/laravel-boolean-softdeletes": "^5.2",
        "voku/portable-utf8": "^6.0"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "composer/composer": "^2.0",
        "filp/whoops": "^2.7",
        "mockery/mockery": "^1.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^11.1.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "secure-http": true,
        "platform": {
            "ext-mongodb": "1.15.0"
        },
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "App\\Wizard\\Steps\\": "app/Steps"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan key:generate --ansi",
            "@php artisan cache:clear",
            "@php artisan config:clear"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "gcp-build": [
            "composer install --no-dev"
        ]
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://apps.dev:VlCQUYB163Z2KWlJ@gitlab.dev.claroshop.com/claroshop/wrapper_dhl.git"
        }
    ]
}
